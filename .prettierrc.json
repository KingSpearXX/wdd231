{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "ignore",
  "cssEnable": [
    "css",
    "scss",
    "less"
  ],
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve"
}